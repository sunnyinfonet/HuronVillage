<table class="table table-bordered">
  <thead "thead-dark">
    <tr>
      <th scope="col">#</th>
      <th scope="col">Item Code</th>
      <th scope="col">Item Name</th>
      <th scope="col">Price</th>
      <th scope="col">Quantity</th>
      <th scope="col">Type</th>
      <th scope="col class="text-end">Action</th>

    </tr>
  </thead>
  <tbody>


    {{#each rows}}

    <tr>
      <th scope="row">{{this.itemID}}</th>
      <td>{{this.itemCode}}</td>
      <td>{{this.itemName}}</td>
      <td>{{this.sellPrice}}</td>
      <td>{{this.stockInhand}}</td>
      <td>{{this.itemType}}</td>
      <td class="text-start">
      <a href="#" type="button" class="btn btn-light btn-small"><i class="bi bi-eye"></i>View</a>
      <a href="/edititem/{{this.itemID}}" type="button" class="btn btn-light btn-small"><i class="bi bi-pencil"></i>Edit</a>
      <a href="/additem/{{this.itemID}}" type="button" class="btn btn-light btn-small"><i class="bi bi-person-x"></i>Delete</a>
    </td>
    </tr>
    {{else}}

    {{/each}}
  </tbody>
</table>